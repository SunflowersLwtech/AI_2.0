# 复制此文件为 .env 并填入你的实际配置

# 你的 Google Cloud 项目信息
GOOGLE_CLOUD_PROJECT=your_project_id
GOOGLE_CLOUD_LOCATION=us-west1

# 你的微调模型端点
VERTEX_AI_ENDPOINT=projects/your_project_id/locations/us-west1/endpoints/your_endpoint_id

# 服务账号 JSON (可以是文件路径或JSON字符串)
GOOGLE_CLOUD_SERVICE_ACCOUNT_JSON=your_service_account_json_here

# Gemini API Key (备用)
GEMINI_API_KEY=your_gemini_api_key_here

# 本地配置
PORT=8000
PYTHON_VERSION=3.11.5
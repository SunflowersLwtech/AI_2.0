# 本地测试环境变量模板
# 复制此文件为 .env 并填入你的实际值

# Google Cloud 项目配置
GOOGLE_CLOUD_PROJECT=your_project_id_here
GOOGLE_CLOUD_LOCATION=us-west1

# 你的微调模型端点 (替换为你的实际端点)
VERTEX_AI_ENDPOINT=projects/your_project_id/locations/us-west1/endpoints/your_endpoint_id

# Google Cloud 服务账号认证
# 方法1: 使用 JSON 文件路径
GOOGLE_CLOUD_SERVICE_ACCOUNT_JSON=/path/to/your/service-account-key.json

# 方法2: 使用 JSON 字符串 (如果你没有文件)
# GOOGLE_CLOUD_SERVICE_ACCOUNT_JSON='{"type":"service_account","project_id":"your_project_id",...}'

# Gemini API Key (备用认证)
GEMINI_API_KEY=your_gemini_api_key_here

# 本地服务器配置
PORT=8000
PYTHON_VERSION=3.11.5

# 开发环境标识
ENVIRONMENT=development